<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link
      rel="preconnect"
      href="https://cdn.shopify.com"
      crossorigin>

    {%- if settings.favicon != blank -%}
      <link
        rel="icon"
        type="image/png"
        href="{{ settings.favicon | img_url: '32x32' }}">
    {%- endif -%}

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link
        rel="preconnect"
        href="https://fonts.shopifycdn.com"
        crossorigin>
    {%- endunless -%}



    <title>{{ page_title }}</title>

    <style>
      [x-cloak] {
        display: none !important;
      }
    </style>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link
      rel="preconnect"
      href="https://fonts.gstatic.com"
      crossorigin />



    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500&display=swap" rel="stylesheet" />



    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css" />



    {{ content_for_header }}


    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    {{ 'style.css' | asset_url | stylesheet_tag }}
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    {{ 'all.min.css' | asset_url | stylesheet_tag }}
    {{ 'main.css' | asset_url | stylesheet_tag }}
    <script defer src="{{ 'jquery.min.js' | asset_url }}"></script>
    <script defer src="{{ 'number.js' | asset_url }}"></script>
    <script defer src="{{ 'bootstrap.bundle.min.js' | asset_url }}"></script>
    <script defer src="{{ 'svg4everybody.min.js' | asset_url }}"></script>
    <script defer src="{{ 'owl.carousel.min.js' | asset_url }}"></script>
    <script defer src="{{ 'nouislider.min.js' | asset_url }}"></script>
    <script defer src="{{ 'main.js' | asset_url }}"></script>

    <script src="{{'VueApp.js' | asset_url }}"></script>
  </head>

  <body>

    <a class="skip-to-content-link button visually-hidden" href="#MainContent">{% comment %} {{ "accessibility.skip_to_text" | t }} {% endcomment %}
    </a>



    {% section 'header' %}
    <div class="site" data-vueapp>
      <div class="side__body">
        {{ content_for_layout }}
      </div>
    </div>



    <script>


      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`
      }
    </script>

